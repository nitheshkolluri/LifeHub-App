    "license": "MIT",
    "dependencies": {
        "@google/genai": "^1.32.0",
        "@google/generative-ai": "^0.24.1",
        "bcrypt": "^5.1.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "firebase-admin": "^12.0.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "redis": "^4.6.12",
        "stripe": "^14.10.0",
