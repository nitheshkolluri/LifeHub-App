{
    "name": "lifehub-backend",
    "version": "1.0.0",
    "description": "LifeHub Backend API - Production-grade REST API for mobile app",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
